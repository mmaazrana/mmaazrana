{
  "experimentalTernaries": true,
  "semi": false,
  "trailingComma": "all",
  "objectWrap": "collapse",
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 100,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "jsxSingleQuote": true,
  "bracketSameLine": false,
  "quoteProps": "as-needed",
  "htmlWhitespaceSensitivity": "strict"
}
